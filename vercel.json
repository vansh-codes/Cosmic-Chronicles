{
    "version": 2,
    "builds": [
        {
            "src": "server.js",
            "use": "@vercel/node"
        }
    ],
    "routes":[
        {
            "src": "/",
            "dest": "/home.html"
        },
        {
            "src": "/blogs",
            "dest": "/blogs.html"
        },
        {
            "src": "/(.+)",
            "dest": "/$1"
        }
    ]
}